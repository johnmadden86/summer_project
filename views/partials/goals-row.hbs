<div class="ui bulleted list">
{{#each member.goals}}
        <tr>
            <td>{{date}}</td>
            <td>{{weight}}</td>
            <td>{{chest}}</td>
            <td>{{thigh}}</td>
            <td>{{upperArm}}</td>
            <td>{{waist}}</td>
            <td>{{hips}}</td>
            <td>{{description}}</td>
            <td>{{status}}</td>
            <td>
                <a href="/goals/{{../member.id}}/delete-goal/{{id}}" class="ui icon button">
                    <i class="icon trash" title="Delete"></i>
                </a>
            </td>
        </tr>
{{/each}}
</div>