<div class="ui bulleted list">
    {{#each assessments}}
        <tr>
            <td>{{date}}</td>
            <td>{{weight}}</td>
            <td>{{chest}}</td>
            <td>{{thigh}}</td>
            <td>{{upperArm}}</td>
            <td>{{waist}}</td>
            <td>{{hips}}</td>
            <td>
                {{#if assessment.trend}}
                    <a class="ui green tag label"></a>
                {{else}}
                    <a class="ui red tag label"></a>
                {{/if}}
            </td>
            <td>{{comment}}</td>
            <td>
                <a href="/dashboard/${_member.id}/deleteassessment/${_assessment.id}" class="ui icon button">
                    <i class="icon trash"></i>
                </a>
            </td>
        </tr>
    {{/each}}
</div>