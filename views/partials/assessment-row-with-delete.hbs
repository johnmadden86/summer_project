<div class="ui bulleted list">
{{#each member.assessments}}
        <tr>
            <td>{{date}}</td>
            <td>{{weight}}</td>
            <td>{{chest}}</td>
            <td>{{thigh}}</td>
            <td>{{upperArm}}</td>
            <td>{{waist}}</td>
            <td>{{hips}}</td>
            <td>
                {{#if trend}}
                    <i class="large green thumbs up icon"></i>
                {{else}}
                    <i class="large red thumbs down icon"></i>
                {{/if}}
            </td>
            <td>{{comment}}</td>
            <td>
                <a href="/dashboard/{{../member.id}}/delete-assessment/{{assessmentId}}" class="ui icon button">
                    <i class="icon trash"></i>
                </a>
            </td>
        </tr>
    <td>{{comment}}</td>
    <td>
        <a href="/dashboard/{{../member.id}}/deleteassessment/{{assessmentId}}" class="ui icon button">
            <i class="icon trash"></i>
        </a>
    </td>
{{/each}}
</div>